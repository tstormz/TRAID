<%= form_for(:customer, :url => @tid_hash.merge({:action => 'results'}), :html=>{:class=>"form-horizontal"}) do |f| %>

  <%= render(:partial => "fields", :locals => {:f => f}) %>

  <%= submit_tag('Search', :class => "btn btn-primary") %>

<% end %>

<%= button_to('Cancel', @return_hash, :class => "btn btn-primary") %>